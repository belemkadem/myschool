<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Student.
    -->
    <changeSet id="20210227063847-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="tutor1_id"
                                 baseTableName="student"
                                 constraintName="fk_student_tutor1_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tutor"/>

        <addForeignKeyConstraint baseColumnNames="tutor2_id"
                                 baseTableName="student"
                                 constraintName="fk_student_tutor2_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tutor"/>

        <addForeignKeyConstraint baseColumnNames="grade_class_id"
                                 baseTableName="student"
                                 constraintName="fk_student_grade_class_id"
                                 referencedColumnNames="id"
                                 referencedTableName="grade_class"/>

    </changeSet>
</databaseChangeLog>
